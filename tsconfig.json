{
    "compilerOptions": {
      "esModuleInterop": true,
      "moduleResolution": "node",
      "jsx": "react-jsx",
      "target": "es2019",
      "module": "commonjs",
      "declaration": true,
      "skipLibCheck": true,
      "outDir": "dist",
      "rootDir": "src",
      "strict": false,
    "noImplicitAny": false
    },
    "include": ["src/**/*"],
  "exclude": ["node_modules", "dist", "**/*.spec.ts"]
  }